{
    "version": 2,
    "name": "svs-marine-angular",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/svs-marine-angular/browser",
                "cleanUrls": true,
                "trailingSlash": false
            }
        }
    ],
    "routes": [
        {
            "src": "^/(.*)",
            "dest": "/index.html",
            "headers": {
                "Cache-Control": "public, max-age=0, must-revalidate",
                "X-Frame-Options": "DENY"
            }
        }
    ]
}
