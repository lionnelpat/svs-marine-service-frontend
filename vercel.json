{
    "version": 2,
    "name": "svs-app",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/svs-app/browser",
                "outputDirectory": "dist/svs-app/browser",
                "cleanUrls": true,
                "trailingSlash": false
            }
        }
    ],
    "routes": [
        {
            "src": "^(?!.*\\.\\w+$).*$",
            "dest": "/index.html",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable",
                "X-Frame-Options": "DENY",
                "Content-Security-Policy": "default-src 'self'"
            }
        },
        {
            "src": "/assets/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31536000, immutable"
            }
        }
    ]
}
