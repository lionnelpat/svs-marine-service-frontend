{
    "version": 2,
    "name": "svs-marine-angular",
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist/svs-marine-angular"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "rewrites": [
        { "source": "/old-path", "destination": "/new-path" }
    ],
    "redirects": [
        { "source": "/temp", "destination": "/permanent", "permanent": true }
    ],
    "headers": [
        { "source": "/(.*)", "headers": [{ "key": "X-Custom-Header", "value": "value" }] }
    ],
    "cleanUrls": true,
    "trailingSlash": false
}
